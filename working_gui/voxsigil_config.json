{
    "vanta_core": {
        "enable_async_bus": true,
        "max_agents": 50,
        "event_timeout": 30.0,
        "debug_mode": true,
        "auto_register_components": true
    },
    "gridformer": {
        "grid_size": 30,
        "num_layers": 6,
        "hidden_dim": 256,
        "num_heads": 8,
        "dropout": 0.1,
        "learning_rate": 0.001
    },
    "novel_reasoning": {
        "logic_units": 100,
        "reasoning_depth": 5,
        "confidence_threshold": 0.8,
        "enable_symbolic_reasoning": true
    },
    "arc_solver": {
        "max_examples": 1000,
        "training_split": 0.8,
        "batch_size": 32,
        "learning_rate": 0.001,
        "max_epochs": 100
    },
    "rag_integration": {
        "embedding_model": "all-MiniLM-L6-v2",
        "chunk_size": 512,
        "overlap": 50,
        "similarity_threshold": 0.7,
        "max_results": 10
    },
    "memory_system": {
        "braid_depth": 10,
        "memory_capacity": 10000,
        "compression_ratio": 0.3,
        "enable_persistent_storage": true
    },
    "training": {
        "epochs": 100,
        "patience": 10,
        "validation_split": 0.2,
        "save_checkpoints": true,
        "checkpoint_frequency": 10
    },
    "gui": {
        "window_size": [1400, 900],
        "enable_dark_mode": false,
        "log_level": "INFO",
        "auto_refresh_interval": 5000
    },
    "paths": {
        "data_dir": "./data",
        "models_dir": "./models", 
        "cache_dir": "./cache",
        "logs_dir": "./logs"
    }
}
